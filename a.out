S_0xec27990 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0xec27b20 .scope module, "tb_decoder_2to4" "tb_decoder_2to4" 3 3;
 .timescale 0 0;
v0xec399f0_0 .var "A", 1 0;
v0xec39ae0_0 .net "Y", 3 0, v0xec398b0_0;  1 drivers
