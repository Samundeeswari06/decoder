    .port_info 1 /OUTPUT 4 "Y";
v0xec28390_0 .net "A", 1 0, v0xec399f0_0;  1 drivers
v0xec398b0_0 .var "Y", 3 0;
E_0xebf0d20 .event anyedge, v0xec28390_0;
    .scope S_0xec02580;
T_0 ;
    %wait E_0xebf0d20;
    %load/vec4 v0xec28390_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0xec398b0_0, 0, 4;
    %jmp T_0.5;
T_0.0 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0xec398b0_0, 0, 4;
    %jmp T_0.5;
T_0.1 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0xec398b0_0, 0, 4;
    %jmp T_0.5;
T_0.2 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0xec398b0_0, 0, 4;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0xec398b0_0, 0, 4;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0xec27b20;
T_1 ;
    %vpi_call/w 3 13 "$monitor", "Time = %0d, A = %b -> Y = %b", $time, v0xec399f0_0, v0xec39ae0_0 {0 0 0};
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0xec399f0_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0xec399f0_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0xec399f0_0, 0, 2;
    %delay 10, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0xec399f0_0, 0, 2;
    %delay 10, 0;
    %vpi_call/w 3 20 "$finish" {0 0 0};
